package com.his.entity;

import java.math.BigDecimal;
import java.util.Date;
/**
 * 	药方表
 * @author 杨培源
 *
 */
public class Prescription {
    private Integer prescriptionDrugId;	//药方主键，自增

    private Integer prescriptionDrugnum;	//药品数量

    private BigDecimal prescriptionTotalprice;	//该药物总价

    private Integer prescriptionPatientId;	//病人编号

    private Integer prescriptionState;	//药方状态
    
    private String prescriptionCode;	//药方编号
    
    private Date prescriptionModifydate;//缴费时间
    
    private Date prescriptionCreatedate;//添加药方的时间
    
    private Drug drug;//药品的对象
    
    public String getPrescriptionCode() {
		return prescriptionCode;
	}

	public void setPrescriptionCode(String prescriptionCode) {
		this.prescriptionCode = prescriptionCode;
	}

	public Integer getPrescriptionDrugId() {
        return prescriptionDrugId;
    }
    
    public void setPrescriptionDrugId(Integer prescriptionDrugId) {
        this.prescriptionDrugId = prescriptionDrugId;
    }
    public Integer getPrescriptionDrugnum() {
        return prescriptionDrugnum;
    }

    public void setPrescriptionDrugnum(Integer prescriptionDrugnum) {
        this.prescriptionDrugnum = prescriptionDrugnum;
    }

    public BigDecimal getPrescriptionTotalprice() {
        return prescriptionTotalprice;
    }

    public void setPrescriptionTotalprice(BigDecimal prescriptionTotalprice) {
        this.prescriptionTotalprice = prescriptionTotalprice;
    }

    public Integer getPrescriptionPatientId() {
        return prescriptionPatientId;
    }

    public void setPrescriptionPatientId(Integer prescriptionPatientId) {
        this.prescriptionPatientId = prescriptionPatientId;
    }

    public Integer getPrescriptionState() {
        return prescriptionState;
    }

    public void setPrescriptionState(Integer prescriptionState) {
        this.prescriptionState = prescriptionState;
    }

	public Drug getDrug() {
		return drug;
	}

	public void setDrug(Drug drug) {
		this.drug = drug;
	}

	public Date getPrescriptionModifydate() {
		return prescriptionModifydate;
	}

	public void setPrescriptionModifydate(Date prescriptionModifydate) {
		this.prescriptionModifydate = prescriptionModifydate;
	}

	public Date getPrescriptionCreatedate() {
		return prescriptionCreatedate;
	}

	public void setPrescriptionCreatedate(Date prescriptionCreatedate) {
		this.prescriptionCreatedate = prescriptionCreatedate;
	}

}