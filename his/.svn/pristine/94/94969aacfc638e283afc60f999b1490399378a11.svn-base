package com.his.entity;

import java.util.Date;
/**
 * 挂号单
 * @author 荣泽尚
 * 
 */
public class Registration {
    private Integer registrationId;

    private Integer registrationPatientId;

    private Integer registrationDepartmentId;

    private Integer registrationStaffId;

    private Date registrationCreatedate;

    private Integer registrationState;

    private Long registrationCost;
    
    private Staff staff;
    private Department department;
    private Patient patient;

    public Integer getRegistrationId() {
        return registrationId;
    }

    public void setRegistrationId(Integer registrationId) {
        this.registrationId = registrationId;
    }

    public Integer getRegistrationPatientId() {
        return registrationPatientId;
    }

    public void setRegistrationPatientId(Integer registrationPatientId) {
        this.registrationPatientId = registrationPatientId;
    }

    public Integer getRegistrationDepartmentId() {
        return registrationDepartmentId;
    }

    public void setRegistrationDepartmentId(Integer registrationDepartmentId) {
        this.registrationDepartmentId = registrationDepartmentId;
    }

    public Integer getRegistrationStaffId() {
        return registrationStaffId;
    }

    public void setRegistrationStaffId(Integer registrationStaffId) {
        this.registrationStaffId = registrationStaffId;
    }

    public Date getRegistrationCreatedate() {
        return registrationCreatedate;
    }

    public void setRegistrationCreatedate(Date registrationCreatedate) {
        this.registrationCreatedate = registrationCreatedate;
    }

    public Integer getRegistrationState() {
        return registrationState;
    }

    public void setRegistrationState(Integer registrationState) {
        this.registrationState = registrationState;
    }

    public Long getRegistrationCost() {
        return registrationCost;
    }

    public void setRegistrationCost(Long registrationCost) {
        this.registrationCost = registrationCost;
    }

	public Staff getStaff() {
		return staff;
	}

	public void setStaff(Staff staff) {
		this.staff = staff;
	}

	public Department getDepartment() {
		return department;
	}

	public void setDepartment(Department department) {
		this.department = department;
	}

	public Patient getPatient() {
		return patient;
	}

	public void setPatient(Patient patient) {
		this.patient = patient;
	}
    
}